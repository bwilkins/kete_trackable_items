<% @title = t('.title', :name => h(@repository.name)) -%>

<h2><%= @title -%></h2>

<ul>
  <%= render @tracking_lists %>
</ul>

<p><%= button_to t('.new'),
       repository_tracking_lists_url(:repository_id => @repository, :method => :post) %></p>

<p><%= link_to t('.back_to', :name => h(@repository.name)), repository_url(:id => @repository) %></p>

